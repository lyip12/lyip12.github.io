        {
            "ID": 226,
            "Name": "Saint Augustine African Orthodox Church and Bishop George A. McGuire",
            "type": "Feature", "geometry": { "type": "Point", "coordinates": [ -71.107646, 42.358441 ]},
            "Ownership": "",
            "AddressNum": 137,
            "Street": "Allston St",
            "City": "Cambridge",
